{
    "name": "raudius/luar",
    "description": "Lua interpreter written entirely in PHP",
    "type": "library",
    "version": "0.1.4",
    "require": {
        "antlr/antlr4-php-runtime": "^0.9.1",
        "ext-json": "*"
    },
    "license": "GPL-2.0-only",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "clean/phpdoc-md": "^0.19.3"
    },
    "authors": [
        {
            "name": "Raul Ferreira Fuentes",
            "email": "r.ferreira.fuentes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raudius\\Luar\\": "src",
            "Raudius\\Luar\\UnitTests\\": "tests/unit"
        }
    },
    "scripts": {
        "test:unit": "phpunit -c tests/phpunit.xml",
        "docs:generate": "phpdoc-md"
    }
}
